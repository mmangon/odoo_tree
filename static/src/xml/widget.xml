<?xml version="1.0" encoding="UTF-8"?>
<templates>
    <t t-name="AgTree">
        <span t-attf-class="oe_form_field {{widget.widget_class}}" t-att-style="widget.attrs.style">
            <div id="tree">
            </div>
        </span>
    </t>

    <t t-name="AgTreeM2O">
        <t t-if="widget.mode != 'readonly'">
            <span t-attf-class="oe_form_field {{widget.widget_class}}" t-att-style="widget.attrs.style">
                <div>
                    <input type="text" class="o_field_char o_field_widget o_input"/>
                </div>
                <div id="tree">
                </div>
            </span>
        </t>
        <t t-if="widget.mode == 'readonly'">
            <a class="o_form_uri o_field_widget o_required_modifier"></a>
        </t>
        
    </t>
</templates>
